{
    "name": "laramore/validations",
    "description": "Add a validation management for Laramore",
    "keywords": ["ORM", "Laravel", "Model", "Migration", "Automatic", "Auto", "Fields", "Field", "Uuid", "Meta", "Table", "Database", "Validations"],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "illuminate/database": ">=5.0",
        "nesbot/carbon": ">=1.0.0",
        "symfony/http-kernel": ">=4.0.0",
        "doctrine/dbal": "^2.9",
        "laramore/laramore": ">=0.2"
    },
    "authors": [
        {
            "name": "Nastuzzi Samy",
            "email": "samy@nastuzzi.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laramore\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpcs"
        ]
    },
    "require-dev": {
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laramore\\Providers\\ValidationsProvider"
            ]
        }
    }
}
